{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "prebuild": "rm -rf ./dist",
    "build": "tsc",
    "prestart": "npm run build && npm run db:restore",
    "start": "node dist/src/server.js",
    "drop": "npx sequelize-cli db:drop",
    "predev": "npm run db:restore",
    "dev": "ts-node-dev src/server.ts",
    "db:restore": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "lint": "eslint --no-inline-config --no-error-on-unmatched-pattern -c .eslintrc.json .",
    "test": "mocha -r ts-node/register ./src/tests/*/*$NAME*.{test,spec}.ts -t 10000 --exit",
    "test:coverage": "nyc npm run test"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.1",
    "sequelize": "^6.33.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.4",
    "@types/chai": "^4.3.9",
    "@types/chai-http": "^4.2.0",
    "@types/express": "^4.17.18",
    "@types/jest": "^29.5.5",
    "@types/jsonwebtoken": "^9.0.3",
    "@types/mocha": "^10.0.3",
    "@types/sequelize": "^4.28.16",
    "@types/sinon": "^10.0.20",
    "@types/sinon-chai": "^3.2.11",
    "@typescript-eslint/eslint-plugin": "^6.7.4",
    "@typescript-eslint/parser": "^6.7.4",
    "chai": "^4.3.10",
    "chai-http": "^4.4.0",
    "eslint": "^8.50.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "jest": "^29.7.0",
    "mocha": "^10.2.0",
    "sequelize-cli": "^6.6.1",
    "sinon": "^17.0.0",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  }
}
